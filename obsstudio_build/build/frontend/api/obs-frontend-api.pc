prefix=/usr/local
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: obs-frontend-api
Description: OBS Studio frontend API library
Version: 32.0.4

Requires:
Libs: -L${libdir} -lobs-frontend-api -lobs
Libs.private: -lobs -lm -pthread
Cflags: -I${includedir} -std=gnu17 -fPIC -fvisibility=hidden -fopenmp-simd -Werror
